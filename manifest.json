{
    "name": "Chrome Time Tracker",
    "version": "1.0",
    "manifest_version": 2,
	"permissions":[
		"storage"
	],
	"background":{
		"persistent":true,
		"scripts":["background.js"]
	},
    "browser_action": {
		"default_popup": "popup.html",
		"default_icon": "icons/blue.png",
		"default_title": "Timing"
    }
}
